<h1>Cocktails#index</h1>
<p>Find me in app/views/cocktails/index.html.erb</p>
<p>
  <ul>
    <% @cocktails.each do |cocktail| %>
      <li>
        <%= link_to cocktail.name, cocktail_path(cocktail.id) %>
        <%= link_to " - [Destroy me]", cocktail_path(cocktail.id), method: :delete, data: { confirm: "Are you sure?" } %>
      </li>
    <% end %>
  </ul>
</p>